{
  "name": "wardrobe/core",
  "description": "The WardrobeCMS core files",
  "keywords": [
    "blog",
      "laravel",
      "cms"
  ],
  "authors": [
    {
      "name": "Eric L Barnes",
      "email": "eric@ericlbarnes.com",
      "role": "Developer"
    },
    {
      "name": "Ryan Tablada",
      "email": "ryan.tablada@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "laravel/framework": "~4",
    "michelf/php-markdown": "1.3.*@dev",
    "symfony/yaml": "2.4.*@dev",
    "intervention/image": "1.4.*"
  },
  "require-dev": {
    "mockery/mockery": "dev-master",
    "phpunit/phpunit": "3.7.*"
  },
  "autoload": {
    "psr-0": {
      "Wardrobe\\Core": "src/",
      "Wardrobe\\Database": "src/"
    }
  },
  "archive": {
    "exclude": [
      "assets"
    ]
  },
  "extra": {
      "branch-alias": {
          "dev-master": "1.1-dev"
      }
  },
  "minimum-stability": "dev",
  "license": "MIT"
}
